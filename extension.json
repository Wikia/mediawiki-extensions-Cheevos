	"ResourceModules": {
		"ext.cheevos.resumable.js": {
			"scripts": [
				"js/cheevos.resumable.js"
			],
			"targets": [
				"desktop",
				"mobile"
			],
			"position": "top"
		},
		"ext.cheevos.dropzone.js": {
			"scripts": [
				"js/cheevos.dropzone.js"
			],
			"targets": [
				"desktop",
				"mobile"
			],
			"position": "top"
		},
		"ext.cheevos.styles": {
			"styles": [
				"css/achievements.css"
			],
			"dependencies": [
				"ext.hydraCore.button",
				"ext.hydraCore.pagination"
			],
			"targets": [
				"desktop",
				"mobile"
			],
			"position": "top"
		},
		"ext.cheevos.js": {
			"scripts": [
				"js/cheevos.js"
			],
			"dependencies": [
				"ext.cheevos.resumable.js"
			],
			"targets": [
				"desktop",
				"mobile"
			],
			"position": "top"
		},
		"ext.cheevos.triggerBuilder.js": {
			"scripts": [
				"js/cheevos.triggerBuilder.js"
			],
			"dependencies": [
				"ext.cheevos.resumable.js"
			],
			"messages": [
				"add_condition",
				"add_trigger",
				"cancel_trigger",
				"delete_condition",
				"delete_trigger",
				"save_trigger",
				"trigger_builder",
				"error_connecting_to_site",
				"bad_site_key",
				"choose_existing_hook"
			],
			"targets": [
				"desktop",
				"mobile"
			],
			"position": "top"
		},
		"ext.cheevos.notice.js": {
			"scripts": [
				"js/cheevos.notice.js"
			],
			"dependencies": [
				"jquery.effects.core",
				"jquery.effects.pulsate"
			],
			"targets": [
				"desktop"
			],
			"position": "top"
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "",
		"remoteExtPath": "Cheevos"
	},
	"Hooks": {
		"ArticleDeleteComplete": "CheevosHooks::onArticleDeleteComplete",
		"PageContentInsertComplete": "CheevosHooks::onPageContentInsertComplete",
		"PageContentSaveComplete": "CheevosHooks::onPageContentSaveComplete",
		"ArticleMergeComplete": "CheevosHooks::onArticleMergeComplete",
		"ArticleProtectComplete": "CheevosHooks::onArticleProtectComplete",
		"BlockIpComplete": "CheevosHooks::onBlockIpComplete",
		"CurseProfileAddComment": "CheevosHooks::onCurseProfileAddComment",
		"CurseProfileAddFriend": "CheevosHooks::onCurseProfileAddFriend",
		"CurseProfileEdited": "CheevosHooks::onCurseProfileEdited",
		"EmailUserComplete": "CheevosHooks::onEmailUserComplete",
		"MarkPatrolledComplete": "CheevosHooks::onMarkPatrolledComplete",
		"UploadComplete": "CheevosHooks::onUploadComplete",
		"WatchArticleComplete": "CheevosHooks::onWatchArticleComplete"
	},
	"callback": "CheevosHooks::onRegistration",
