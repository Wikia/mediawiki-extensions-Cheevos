		"Cheevos": [
			"i18n"
		]
	},
	"ExtensionMessagesFiles": {
		"SpecialCheevos": "Cheevos.alias.php"
	},
	"AutoloadClasses": {
		"CheevosHooks": "Cheevos.hooks.php",
		"Cheevos\\dataMiner": "classes/dataMiner.php",
		"Cheevos\\Cheevos": "classes/Cheevos.php",
		"Cheevos\\CheevosHelper": "classes/CheevosHelper.php",
		"Cheevos\\CheevosException": "classes/CheevosException.php",
		"Cheevos\\CheevosModel": "classes/CheevosModel.php",
		"Cheevos\\CheevosAchievement": "classes/CheevosAchievement.php",
		"Cheevos\\CheevosAchievementCategory": "classes/CheevosAchievementCategory.php",
		"Cheevos\\CheevosAchievementCriteria": "classes/CheevosAchievementCriteria.php",
		"Cheevos\\CheevosStatDelta": "classes/CheevosStatDelta.php",
		"Cheevos\\CheevosStatProgress": "classes/CheevosStatProgress.php",
		"SpecialManageAchievements": "specials/SpecialManageAchievements.php",
		"TemplateManageAchievements": "templates/TemplateManageAchievements.php"
